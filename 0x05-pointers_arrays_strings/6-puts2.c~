#include "main.h"

/**
 *puts2 - prints every other character of a string,
 * starting with the first character
 * @str: string to be worked on
 * Return: empty
 */
void puts2(char *str)
{
int i;
int j = 0;

while (str[j] != '\0')
{
j++;
}

for (1 = 0, i < j; i += 2)
{
_putchar(str[i]);
}
_putchar('\n');
}
